{
  "name": "vue-use-screen-orientation",
  "version": "0.0.0",
  "packageManager": "pnpm@8.6.1",
  "description": "A Vue composable for detecting screen orientation using media query.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/vue-use-screen-orientation#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/vue-use-screen-orientation.git"
  },
  "bugs": "https://github.com/wobsoriano/vue-use-screen-orientation/issues",
  "keywords": ["vue", "hooks"],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "pnpm build-fast --dts-resolve",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "update-deps": "taze -w && pnpm i",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.0",
    "vue": "^2.6.14 || ^3.3.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "vue-demi": ">=0.14.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "bumpp": "^9.1.1",
    "eslint": "^8.42.0",
    "taze": "^0.10.2",
    "tsup": "6.7.0",
    "typescript": "^5.1.3",
    "vitest": "^0.32.0",
    "vue": "^3.3.4"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
